    "@types/node": "^22.10.2",
    "@types/bcrypt": "^5.0.2",
    "@types/passport-jwt": "^4.0.1",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^8.57.1",
    "prisma": "^6.1.0",
    "supertest": "^7.0.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
